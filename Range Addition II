class Solution:
		def maxCount(self, m: int, n: int, ops: List[List[int]]) -> int:
			if not ops:
				return m * n
		
			# Find the minimum a and b across all operations
			min_a = min(op[0] for op in ops)
			min_b = min(op[1] for op in ops)
			
			# The maximum value will be in the rectangle from (0,0) to (min_a-1, min_b-1)
			return min(min_a, m) * min(min_b, n)

